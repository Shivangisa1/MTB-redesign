{
  "name": "project1",
  "version": "1.0.0",
  "description": "Playwright tests for Project 1",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-typescript": "^7.27.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.3",
    "@types/nodemailer": "^7.0.4",
    "allure-commandline": "^2.36.0",
    "allure-playwright": "^3.4.3",
    "chokidar": "^4.0.3",
    "cross-spawn": "^7.0.6",
    "istanbul": "^0.4.5",
    "nyc": "^17.1.0",
    "open": "^10.2.0",
    "serve": "^14.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "test": "npx playwright test",
    "allure:clean": "if exist allure-results rmdir /s /q allure-results & if exist allure-report rmdir /s /q allure-report",
    "allure:generate": "allure generate ./allure-results --clean -o ./allure-report",
    "allure:open": "allure open ./allure-report",
    "test:allure": "npm run allure:clean && npm run test && npm run allure:generate && npm run allure:open"
  },
  "dependencies": {
    "nodemailer": "^7.0.10",
    "xlsx": "^0.18.5"
  },
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
